{
  "name": "gloord/delta2json",
  "description": "",
  "keywords": ["DELTA", "parser", "delta2json"],
  "license": "MIT",

  "require": {
    "php": "^7.2",
    "symfony/console": "^4.1",
    "symfony/filesystem": "^4.1"
  },

  "autoload": {
    "psr-4": {
      "Gloord\\DeltaParser\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },

  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },

  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },

  "bin": ["bin/delta2json"],

  "minimum-stability": "dev",
  "prefer-stable": true
}
